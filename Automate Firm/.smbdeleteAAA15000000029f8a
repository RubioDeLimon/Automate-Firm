        }
        else if (payOTFlag==14)
        {
            if (dropTableFlag==0) {
                if (indexPath.row==5) {
                    payOTTextFlag=1;
                    
                    self.payOTTC.hidden=true;
                }
                else
                {
                    
                    self.selectedFlag1=0;
                    self.payOTTC.hidden=true;
                }
                
            }

