                    [self.holidayPayOTText3 becomeFirstResponder];
                    self.payOTTC.hidden=true;
                }
                else
                {
                    self.holidayPayOTText3.text=[self.payOTDisplayArray1 objectAtIndex: indexPath.row];
                    self.selectedFlag1=0;
                    self.payOTTC.hidden=true;
                }
            }
            
        }
        else if (payOTFlag==13)
        {
            if (dropTableFlag==0) {
                if (indexPath.row==5) {
                    payOTTextFlag=1;
